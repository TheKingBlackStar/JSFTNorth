{
  "functions": {
    "source": "JSFTNorth-main/functions"
  },
  "firestore": {
    "rules": "JSFTNorth-main/firestore.rules",
    "indexes": "JSFTNorth-main/firestore.indexes.json"
  },
  "storage": {
    "rules": "storage.rules"
  }
  ,"hosting": {
    "public": "JSFTNorth-main",
    "ignore": [
      "firebase.json",
      "**/.*",
      "**/node_modules/**"
    ],
    "rewrites": [
      {
        "source": "**",
        "destination": "/index.html"
      }
    ]
  }
}
